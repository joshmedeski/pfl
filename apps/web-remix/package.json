{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "storybook": "concurrently \"npm run dev:css\" \"start-storybook -p 6006\"",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.5",
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "@remix-run/server-runtime": "^1.2.3",
    "@remix-run/vercel": "^1.1.1",
    "@types/react-inner-image-zoom": "^3.0.0",
    "axios": "^0.25.0",
    "classnames": "^2.3.1",
    "dotenv": "^14.2.0",
    "moviestmdb": "^2.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-inner-image-zoom": "^3.0.0",
    "remix": "^1.1.1",
    "tiny-invariant": "^1.2.0",
    "tmdb": "workspace:*",
    "ui": "workspace:*",
    "use-debounce": "^7.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.10",
    "@remix-run/dev": "^1.1.1",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/react": "^6.4.14",
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "raml-to-typescript": "^0.1.1",
    "storybook-addon-tw-dm-toggle": "^1.0.4",
    "storybook-dark-mode": "^1.0.8",
    "storybook-tailwind-dark-mode": "^1.0.11",
    "tailwindcss": "^3.0.15",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
